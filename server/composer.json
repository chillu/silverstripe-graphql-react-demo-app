{
    "name": "chillu/silverstripe-graphql-react-demo",
    "type": "silverstripe-recipe",
    "description": "SilverStripe + GraphQL",
    "require": {
        "php": ">=5.6.0",
        "silverstripe/recipe-plugin": ">=0.1@dev <1.0",
        "silverstripe/recipe-core": "1.1.x-dev",
        "silverstripe/admin": "1.1.x-dev",
        "silverstripe/asset-admin": "1.1.x-dev",
        "silverstripe/graphql": "1.1.x-dev",
        "silverstripe/versioned": "1.1.x-dev",
        "silverstripe/serve": "dev-master",
        "silverstripe/sqlite3": "dev-master"
    },
    "require-dev": {
        "phpunit/PHPUnit": "^5.7",
        "silverstripe/graphql-devtools": "dev-master"
    },
    "extra": {
        "project-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "mysite/.htaccess",
            "mysite/_config.php",
            "mysite/_config/mysite.yml",
            "mysite/code/Page.php",
            "mysite/code/PageController.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "scripts": {
        "serve": "serve --port 3001"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
